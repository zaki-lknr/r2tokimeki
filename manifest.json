{
    "manifest_version": 3,
    "name": "r2tokimeki",
    "version": "0.1.0",
    "description": "Link To TOKIMEKI",
    "icons": {
        "128": "icons/r2tokimeki.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "r2tokimeki.js"
            ],
            "run_at": "document_end"
        }
    ],
    "omnibox": {
        "keyword": "r2t"
    },
    "background": {
        "service_worker": "search.js"
    }
}
