{
    "manifest_version": 3,
    "name": "r2tokimeki",
    "version": "0.1.0",
    "description": "Open Bluesky-Link on TOKIMEKI <https://tokimeki.blue/>",
    "icons": {
        "128": "icons/r2tokimeki.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "r2tokimeki.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {},
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "omnibox": {
        "keyword": "r2t"
    },
    "background": {
        "service_worker": "worker.js"
    }
}
